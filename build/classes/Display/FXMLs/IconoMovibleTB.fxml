<fx:root fx:id="root_pane" minWidth="-Infinity" prefHeight="0.0" prefWidth="100.0" styleClass="dragicon" stylesheets="@Aplicacion.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
